<!-- modal -->
<div class="cont">
  <!-- Button trigger modal -->
  <a class="btn_gold_in_card" data-toggle="modal" data-target="#exampleModal">VOIR LES PHOTOS <span><i class="fas fa-long-arrow-alt-right"></i></span></a>
  <!-- Modal -->
  <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-body" style="padding: 0 !important">
          <!-- bouton close -->
          <div id="close_modal">
            <button type="button" class="close button_close_modal" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </div>
          <!-- carrousel -->
          <div id="carouselExampleIndicators" class="carousel slide cont_carousel" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
              <div class="carousel-item active">
                <!-- <img class="carousel_img" src="/images/cookoon-champagne.png" alt="First slide"> -->
                <%= image_tag 'cookoon-champagne.png', alt:'First slide', class: 'carousel_img' %>
              </div>
              <div class="carousel-item">
                <!-- <img class="carousel_img" src="/images/cookoon-diner-1.png" alt="Second slide"> -->
                <%= image_tag 'cookoon-diner-1.png', alt:'Second slide', class: 'carousel_img' %>
              </div>
              <div class="carousel-item">
                <!-- <img class="carousel_img" src="/images/cookoon-chef-1.png" alt="Third slide"> -->
                <%= image_tag 'cookoon-chef-1.png', alt:'Third slide', class: 'carousel_img' %>
              </div>
            </div>
            <a class="carousel-control-prev control-margin" href="#carouselExampleIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next control-margin" href="#carouselExampleIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

