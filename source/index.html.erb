<!-- CARD DOUBLE LES CHEFS -->
<div class="cont">
  <!-- carte double image à droite-->
  <div class="card_double_img_on_right">
    <!-- carte description -->
    <div class="card_description padding-left">
      <h1 class="card_title">Les chefs</h1>
      <div class="bande_horizontale"></div>
      <p class="card_text">Etoilés, issus de grandes maisons ou de célèbres émissions, les chefs vous font vivre une expérience gastronomique exclusive. À noter : certains d'entre eux se réservent uniquement par l'intermédiaire de la conciergerie, plusieurs mois à l'avance.</p>
      <!-- bouton lien vers typeform -->
<!--       <a class="btn_gold_in_card" id="ID6" href="https://cookoon.typeform.com/to/NpySLO" target="_blank">Cuisiner avec nous <span><i class="fas fa-long-arrow-alt-right"></i></span>
      </a> -->

      <!-- modal carousel -->
      <!-- Button trigger modal -->
      <button id="ID6" class="btn_gold_in_card" data-toggle="modal" data-target="#chefsModal">Voir les chefs <span><i class="fas fa-long-arrow-alt-right"></i></span></button>

      <!-- Modal -->
      <div class="modal fade" id="chefsModal" tabindex="-1" role="dialog" aria-labelledby="chefsModalLabel" aria-hidden="true">
        <div class="modal-dialog" id="modal-dialog-chef" role="document">
          <div class="modal-content">
            <div class="modal-body">

              <!-- bouton close -->
              <button class="close" data-dismiss="modal" aria-label="Close">
                <i class="fas fa-times"></i>
              </button>

              <!-- indicateur de slide -->
              <div id="carouselChefsIndicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                  <li data-target="#carouselChefsIndicators" data-slide-to="0" class="active"></li>
                  <li data-target="#carouselChefsIndicators" data-slide-to="1"></li>
                  <li data-target="#carouselChefsIndicators" data-slide-to="2"></li>
                  <li data-target="#carouselChefsIndicators" data-slide-to="3"></li>
                </ol>

                <!-- emplacement photos -->
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <div class="figure">
                      <img class="d-block img-chef" src="images/cookoon-chef-2.jpg" alt="Baptiste Renouard">
                      <figcaption class="chef-figcaption">Baptiste Renouard</figcaption>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <div class="figure">
                      <img class="d-block img-chef" src="images/cookoon-chef-1.png" alt="Agnès Wiriath">
                      <figcaption class="chef-figcaption">Agnès Wiriath</figcaption>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <div class="figure">
                      <img class="d-block img-chef" src="images/cookoon-chef-3.jpg" alt="Dominique Boucher">
                      <figcaption class="chef-figcaption">Dominique Boucher</figcaption>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <div class="figure">
                      <img class="d-block img-chef" src="images/cookoon-chef-4.jpg" alt="François Marchenay">
                      <figcaption class="chef-figcaption">François Marchenay</figcaption>
                    </div>
                  </div>
                </div>

                <!-- flèches pour changer de photo -->
                <a class="carousel-control-prev" href="#carouselChefsIndicators" role="button" data-slide="prev">
                  <i class="fas fa-chevron-left carrousel-chevron-left"></i>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselChefsIndicators" role="button" data-slide="next">
                  <i class="fas fa-chevron-right carrousel-chevron-right"></i>
                  <span class="sr-only">Next</span>
                </a>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- carte 4 images -->
    <div class="card_four_images">
      <div class="bloc_img_txt">
        <div class="img_chef">
          <%= image_tag 'cookoon-chef-2.jpg', alt:'Baptiste Renouard', class:'card_four_images_img one' %>
        </div>
        <div class="chef_name">
          <h1>Baptiste Renouard</h1>
        </div>
      </div>
      <div class="bloc_img_txt">
        <div class="img_chef">
          <%= image_tag 'cookoon-chef-1.png', alt:'Agnès Wiriath', class:'card_four_images_img two' %>
        </div>
        <div class="chef_name">
          <h1>Agnès Wiriath</h1>
        </div>
      </div>
      <div class="bloc_img_txt">
        <div class="img_chef">
          <%= image_tag 'cookoon-chef-3.jpg', alt:'Dominique Boucher', class:'card_four_images_img three' %>
        </div>
        <div class="chef_name">
          <h1>Dominique Boucher</h1>
        </div>
      </div>
      <div class="bloc_img_txt">
        <div class="img_chef">
          <%= image_tag 'cookoon-chef-4.jpg', alt:'François Marchenay', class:'card_four_images_img four' %>
        </div>
        <div class="chef_name">
          <h1>François Marchenay</h1>
        </div>
      </div>
    </div>
  </div>
</div>



