                  <div class="carousel-item active">
                    <img class="d-block" src="images/cookoon-spot-7.jpg" alt="First slide">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block" src="images/cookoon-spot-5.jpg" alt="Second slide">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block" src="images/cookoon-spot-4.jpg" alt="Third slide">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block" src="images/cookoon-spot-6.jpg" alt="Fourth slide">
                  </div>
                </div>

                <!-- flèches pour changer de photo -->
                <a class="carousel-control-prev" href="#carouselplacesIndicators" role="button" data-slide="prev">
